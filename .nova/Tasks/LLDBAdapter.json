{
  "actions" : {
    "build" : {
      "postActions" : [
        {
          "name" : "LLDBAdapter",
          "script" : "pushd DebugAdapter\nswift build --product LLDBAdapter\npopd\n",
          "type" : "runScript"
        }
      ]
    },
    "clean" : {
      "postActions" : [
        {
          "name" : "LLDBAdapter",
          "script" : "pushd DebugAdapter\nswift package clean\npopd\n",
          "type" : "runScript"
        }
      ]
    }
  },
  "buildBeforeRunning" : true,
  "extension" : {
    "identifier" : "panic.Icarus",
    "name" : "Icarus"
  },
  "extensionTemplate" : "lldbDebug",
  "extensionValues" : {
    "launchPath" : "DebugAdapter\/.build\/debug\/LLDBAdapter",
    "request" : "attach"
  }
}
